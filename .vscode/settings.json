{
    "rust-analyzer.rustfmt.overrideCommand": ["leptosfmt", "--stdin", "--rustfmt"],
    "tailwindCSS.includeLanguages": {
        "rust": "html",
        "*.rs": "html"
    },
    "conventionalCommits.scopes": [
        "workspace"
    ],
    "tailwindCSS.experimental.classRegex": [
        ["class_names!\\([^)]*\\)", "\"[^\"]\""]
    ]
}